{
  "author": "riya-amemiya",
  "bugs": {
    "url": "https://github.com/riya-amemiya/UMT/issues"
  },
  "description": "個人的に作った便利関数集 個人用途なので破壊的変更がある可能性があります。 Main Packageに依存関係はありません。 JavaScriptからの呼び出しに対応しています。",
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@swc/core": "^1.3.100",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "bun-types": "^1.0.15",
    "eslint": "^8.55.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "gh-pages": "^6.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2",
    "umt": "^1.3.6"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "tests"
  },
  "homepage": "https://github.com/riya-amemiya/UMT#readme",
  "keywords": [],
  "license": "MIT",
  "main": "module/index.js",
  "types": "module/index.d.js",
  "files": [
    "module",
    "!module/tests",
    "!module/tsconfig.tsbuildinfo"
  ],
  "name": "umt",
  "repository": {
    "type": "git",
    "url": "git://github.com/riya-amemiya/UMT.git"
  },
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "clean-build": "rm -rf module && yarn build",
    "deploy": "yarn typedoc && gh-pages -d doc",
    "test": "jest",
    "test-debug": "cd test && tsc",
    "ts-node": "yarn build && ts-node --project tmp/tsconfig.json tmp/src/index.ts",
    "lint": "yarn eslint --fix && biome check . && tsc",
    "format": "biome format . --write",
    "eslint": "eslint src"
  },
  "version": "1.4.1"
}
